{
    "name": "ixocreate/cms",
    "description": "IXOCREATE CMS implementation",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "symfony/options-resolver": "^4.1",
        "ixocreate/contract": "dev-develop",
        "thepixeldeveloper/sitemap": "^5.1"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0.1",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^6.0.6"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Cms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateMisc\\Cms\\": "tests/misc/",
            "IxocreateTest\\Cms\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
